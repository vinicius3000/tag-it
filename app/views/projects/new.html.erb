<h1>New project</h1>


<%= form_for(Project.new, :html => { :class => 'form-vertical', :multipart => true }) do |f| %>
   <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :goal %><br>
    <%= f.text_area :goal %>
  </p>

  <%= f.file_field :video %>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>


<%= render 'form' %>

<%= link_to 'Back', projects_path %>



